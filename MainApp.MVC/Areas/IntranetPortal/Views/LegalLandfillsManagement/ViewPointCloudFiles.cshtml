                    });
                }
            });
        }
        function deleteFileModalFunction(fileId) {
            $.ajax({
                type: "POST",
                url: "@Url.Action("GetLegalLandfillPointCloudFileById", "LegalLandfillsManagement", new { Area = "IntranetPortal" })",
                data: {
                    fileId: fileId
                },
                success: function (data) {
                    if (data != undefined || data != null) {
                        if (data.isSuccess == false && data.errMsg != null) {
                            Swal.fire({
                                text: `${data.errMsg}`,
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "error"
                            });
                        }
                        else {
                            $("#deleteLegalLandfillFileModalBody").html("");
                            var sdt = new Date(data.data.scanDateTime);
                            var formatedDate = `${sdt.getDate()}.${sdt.getMonth() + 1}.${sdt.getFullYear()}`;                                                       

                            $("#deleteLegalLandfillFileModalBody").append(
                                '<div>'
                                + '<input type="text" id="txtDeleteFileId" hidden/>'
                                + '<input type="text" id="txtDeleteFileName" hidden/>'
                                + '<input type="text" id="txtDeleteScanDate" hidden/>'

                                + '<table class="table table-striped my-4 w-100">'
                                + '<thead>'
                                + '<tr>'
                                + '<th>@DbResHtml.T("Name", "Resources")</th>'
                                + '<th>@DbResHtml.T("Scan Date", "Resources")</th>'
                                + '</tr>'
                                + '</thead>'
                                + '<tbody>'
                                + '<tr>'
                                + `<td>${data.data.fileName}</td>`
                                + `<td>${formatedDate}</td>`
                                + '</tr>'
                                + '</tbody>'
                                + '</table>'
                                + '</div>'
                            );

                            $(`#txtDeleteFileId`).val(data.data.id);
                            $(`#txtDeleteFileName`).val(data.data.fileName);
                            $(`#txtDeleteScanDate`).val(data.data.scanDateTime);

                            $('#deleteLegalLandfillFileModal').modal('show');
                        }
                    }
                    else {
                        Swal.fire({
                            text: `@DbResHtml.T("No data received", "Resources")`,
                            confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                            allowOutsideClick: false,
                            icon: "error"
                        });
                    }
                },
                error: function (req, status, error) {
                    ajaxErrorHandlingAlert("error", req.status);
                }
            });
        }
        function previewSelectedFiles(){
            var selectedFiles = [];
            $("input[type='checkbox']:checked").each(function () {
                var checkboxId = $(this).attr('id');
                var itemId = checkboxId.split('_')[1];
                selectedFiles.push(itemId);
            });

            if (selectedFiles.length === 0) {
                Swal.fire({
                    icon: "warning",
                    text: "@DbResHtml.T("No files selected", "Resources")"
                });
                return;
            }

            $.ajax({
                type: "POST",
                url: "@Url.Action("ProcessSelectedFiles", "LegalLandfillsManagement", new { Area = "IntranetPortal" })",
                data: JSON.stringify(selectedFiles),
                contentType: "application/json",
                success: function (data) {
                    if (data != undefined || data != null) {
                        if (data.isSuccess == false && data.errMsg != null) {
                            Swal.fire({
                                text: `${data.errMsg}`,
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "error"
                            });
                        }
                        else {
                            window.location.href = data.data;
                        }
                    }
                    else {
                        Swal.fire({
                            text: `@DbResHtml.T("No data received", "Resources")`,
                            confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                            allowOutsideClick: false,
                            icon: "error"
                        });
                    }
                },
                error: function (xhr, status, error) {
                    Swal.fire({
                        icon: "error",
                        text: "@DbResHtml.T("Error occurred while processing your request", "Resources")"
                    });
                }
            });
        }
        function getAllLegalLandfills() {
            $("#legalLandfillSelect").empty();
            $.ajax({
                type: "POST",
                url: "@Url.Action("GetAllLegalLandfills", "LegalLandfillsManagement", new { Area = "IntranetPortal" })",
                success: function (data) {
                    if (data != undefined || data != null) {
                        if (data.isSuccess == false && data.errMsg != null) {
                            Swal.fire({
                                text: `${data.errMsg}`,
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "error"
                            });
                        }
                        else {
                            $(data.data).each(function (index, item) {
                                $("#legalLandfillSelect").append($('<option></option>').val(item.id).html(item.name));
                                if ($("#editLegalLandfillSelect option[value='" + item.id + "']").length == 0) {
                                    $("#editLegalLandfillSelect").append($('<option></option>').val(item.id).html(item.name));
                                }
                                
                            });
                        }
                    }
                    else {
                        Swal.fire({
                            text: `@DbResHtml.T("No data received", "Resources")`,
                            confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                            allowOutsideClick: false,
                            icon: "error"
                        });
                    }
                },
                error: function (req, status, error) {
                    Swal.fire({
                        text: '@DbResHtml.T("Error occured", "Resources")',
                        icon: "error"
                    });
                }
            });
        }
        function uploadConvertFileConfirmed() {
            var date = getDefaultFormatDate('#dateOfScan');
            if ($("#addPointCloudFilelNameInput").val() == '' || date == '' || $('#legalLandfillSelect').val() == '' || $('#addPointCloudFileInput')[0].files.length === 0) {
                Swal.fire({
                    icon: "warning",
                    text: "@DbResHtml.T("All fields are required", "Resources")",
                    allowOutsideClick: false
                });
                return;
            }
            Swal.fire({
                icon: "warning",
                text: "@DbResHtml.T("Uploading/Converting in progress...", "Resources")",
                allowOutsideClick: false,
                showConfirmButton: false
            });
            var formData = new FormData($('#uploadConvertFileForm')[0]);

            $.ajax({
                type: "POST",
                url: "@Url.Action("UploadConvertLegalLandfillPointCloudFileConfirmed", "LegalLandfillsManagement", new { Area = "IntranetPortal" })",
                data: formData,
                async: true,
                processData: false,
                contentType: false,
                success: function (data) {
                    if (data != undefined || data != null) {
                        if (data.isSuccess == false && data.errMsg != null) {
                            Swal.fire({
                                text: `${data.errMsg}`,
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "error"
                            });
                        }
                        else {
                            Swal.fire({
                                text: '@DbResHtml.T("Successfully uploaded and converted file", "Resources")',
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "success"
                            }).then((result) => {
                                if (result.isConfirmed) {
                                    location.reload();
                                }
                            });
                        }
                    }
                    else {
                        Swal.fire({
                            text: `@DbResHtml.T("No data received", "Resources")`,
                            confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                            allowOutsideClick: false,
                            icon: "error"
                        });
                    }
                },
                error: function (req, status, error) {
                    Swal.fire({
                        text: '@DbResHtml.T("Error occured", "Resources")',
                        icon: "error"
                    });
                }
            });


        }
    </script>

}
@section BodyArea {
    <div class="modal" id="wasteVolumeDiffAnalysisModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header bg-gradient-secondary">
                    <h4 class="modal-title">@DbResHtml.T("Waste volume differential analysis", "Resources")</h4>                   
                </div>
                <div id="wasteVolumeDiffAnalysisModalBody" class="modal-body">
                </div>
                <div class="modal-footer">
                    <button type="button" class="mb-1 btn bg-gradient-secondary" data-dismiss="modal" aria-label="Ok">
                         @DbResHtml.T("Ok", "Resources")
                    </button>
                    
                </div>
