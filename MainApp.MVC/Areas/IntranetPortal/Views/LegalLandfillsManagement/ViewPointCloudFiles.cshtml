@using MainApp.MVC.Helpers
@using MainApp.MVC.ViewModels.IntranetPortal.LegalLandfillManagement
@using Microsoft.AspNetCore.Localization
@using SD.Helpers
@model IEnumerable<LegalLandfillPointCloudFileViewModel>

@{
    ViewData["Title"] = @DbResHtml.T("Legal landfill point cloud files list", "Resources");
}

<div class="content-header bg-gradient-white border-bottom mb-4">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6">
                <h4 class="text-muted mb-0"> @ViewData["Title"]</h4>
                <small class="text-muted">@DbResHtml.T("Upload/Convert, edit, delete and view all legal landfill point cloud files", "Resources")</small>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#" class="text-success">@DbResHtml.T("Home", "Resources")</a></li>
                    <li class="breadcrumb-item active">@ViewData["Title"]</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">
                        @if (User.HasAuthClaim(SD.AuthClaims.UploadConvertLegalLandfillPointCloudFile))
                        {
                            <button class="mb-1 btn bg-gradient-primary float-left" data-toggle="modal" data-target="#uploadConvertFileModal" title=@DbResHtml.T("Upload/Convert file", "Resources")>
                                <i class="fas fa-plus"></i>
                                @DbResHtml.T("Upload/Convert file", "Resources")
                            </button>
                        }
                        @if (User.HasAuthClaim(SD.AuthClaims.PreviewLegalLandfillPointCloudFiles))
                        {
                            <button id="previewPointCloudFileBtn" onclick="previewSelectedFiles()" class="mb-1 ml-2 btn bg-gradient-info float-left" title=@DbResHtml.T("View all of the selected point cloud files", "Resources")>
                                <i class="fas fa-info-circle"></i>
                                @DbResHtml.T("View Point Clouds", "Resources")
                            </button>
                        }
                        @if (User.HasAuthClaim(SD.AuthClaims.ViewWasteVolumeDiffAnalysis))
                        {
                            <button id="wasteVolumeDiffAnalysisBtn" onclick="wasteVolumeDiffAnalysisFunc()" class="mb-1 ml-2 btn bg-gradient-secondary float-left" title=@DbResHtml.T("Differential analysis of waste volume", "Resources")>
                                <i class="fas fa-ruler-combined"></i>
                                @DbResHtml.T("Waste volume differential analysis", "Resources")
                            </button>
                        }
                        <button id="exportToExcelButton" class="btn btn-success mb-1 float-right" type="button" title="Export to Excel">
                            <i class="fas fa-file-alt"></i>
                        </button>

                        <div class="mt-5">
                            <table id="legalLandfillPointCloudFilesTable" class="table table-sm table-bordered table-striped custom-responsive-table">
                                <thead>
                                    <tr>
                                        <th hidden></th>
                                        <th>
                                            @DbResHtml.T("Name", "Resources")
                                        </th>
                                        <th>
                                            @DbResHtml.T("Scan Date Time", "Resources")
                                        </th>
                                        <th>
                                            @DbResHtml.T("Legal Landfill", "Resources")
                                        </th>
                                        <th>
                                            @DbResHtml.T("Select To Preview", "Resources")
                                        </th>
                                        <th class="notexport">
                                            @DbResHtml.T("Action", "Resources")
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td hidden>@item.Id</td>
                                            <td>@item.FileName</td>
                                            <td>@item.ScanDateTime.ToString("dd.MM.yyyy")</td>
                                            <td>@item.LegalLandfill?.Name</td>
                                            <td>
                                                <div class="icheck-info">
                                                    <input type="checkbox" id="previewFile_@item.Id" name="previewFile_@item.Id" data-id="@item.Id">
                                                    <label for="previewFile_@item.Id">
                                                    </label>
                                                </div>
                                            </td>
                                            <td>
                                                @if (User.HasAuthClaim(SD.AuthClaims.EditLegalLandfillPointCloudFile))
                                                {
                                                    <button class="mb-1 btn bg-gradient-warning text-white btn-xs" id="@item.Id" onclick="editFileModalFunction('@item.Id')" title=@DbResHtml.T("Edit", "Resources")>
                                                        <i class="fas fa-edit"></i>
                                                    </button>
                                                }
                                                @if (User.HasAuthClaim(SD.AuthClaims.DeleteLegalLandfillPointCloudFile))
                                                {
                                                    <button  class="ml-1 mb-1 btn bg-gradient-danger btn-xs" onclick="deleteFileModalFunction('@item.Id')" title="@DbResHtml.T("Delete", "Resources")">
                                                        <i class="fas fa-trash"></i>
                                                    </button>
                                                }
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script>
        @{
            var httpContext = ViewContext.HttpContext;
            var currentCulture = httpContext.Features.Get<IRequestCultureFeature>().RequestCulture.UICulture.TwoLetterISOLanguageName;
            var langPath = Url.Content("~/i18n/datatable/" + @currentCulture + ".json");
        }
            $(document).ready(function () {
                $('#legalLandfillPointCloudFilesTable').DataTable({
                    paging: true,
                    pageLength: 100,
                    "columnDefs": [
                        { "width": "15%", "targets": 2 }
                    ],
                    "language": {
                        "url": "@langPath"
                    },
                    dom: 'Bfrtip',
                    responsive: true,
                    buttons: [
                        {
                            extend: 'excelHtml5',
                            className: 'btn bg-gradient-success border-0 d-none export-to-excel-button',
                            text: '<i class="fas fa-file-alt"></i>',
                            titleAttr: "Export to excel",
                            filename: "Legal landfill cloud files - @DateTime.Now.ToString("dd.MM.yyyy")",
                            exportOptions: {
                                columns: ':not(.notexport)'
                            }
                        }
                    ]
                });
                $("#legalLandfillSelect").select2({
                    width: '100%',
                    theme: 'bootstrap4'
                });               
                $('#dateOfScan').datetimepicker({
                    format: 'L'
                });
                getAllLegalLandfills();
                $("#exportToExcelButton").click(function () {
                    $(".export-to-excel-button").click();
                });


            });


        function wasteVolumeDiffAnalysisFunc() {
            var selectedFiles = [];
            $("input[type='checkbox']:checked").each(function () {
                var checkboxId = $(this).attr('id');
                var itemId = checkboxId.split('_')[1];
                selectedFiles.push(itemId);
            });

            if (selectedFiles.length === 0) {
                Swal.fire({
                    icon: "warning",
                    text: "@DbResHtml.T("No files selected", "Resources")"
                });
                return;
            }

            if (selectedFiles.length === 1 || selectedFiles.length > 2) {
                Swal.fire({
                    icon: "warning",
                    text: "@DbResHtml.T("Choose only two files", "Resources")"
                });
                return;
            }

            $.ajax({
                type: "POST",
                url: "@Url.Action("WasteVolumeDiffAnalysis", "LegalLandfillsManagement", new { Area = "IntranetPortal" })",
                data: JSON.stringify(selectedFiles),
                contentType: "application/json",
                success: function (data) {
                    console.log(data);
                    if (data != undefined || data != null) {
                        if (data.isSuccess == false && data.errMsg != null) {
                            Swal.fire({
                                text: `${data.errMsg}`,
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "error"
                            });
                        }
                        else {
                            $("#wasteVolumeDiffAnalysisModalBody").empty();
                            
                            var html = `<div class="info-box">
                                            <span class="info-box-icon bg-secondary"><i class="fas fa-chart-bar"></i></span>
                                            <div class="info-box-content">
                                                <span class="info-box-text">File 1:
                                                    <br>
                                                    <label class="btn btn-default text-center font-weight-light">
                                                        <span class="font-weight-normal">${data.data.fileAName}</span> /
                                                        ${data.data.scanDateFileA}
                                                    </label>                                                   
                                                    <br>
                                                    File 2:
                                                    <br>
                                                    <label class="btn btn-default text-center font-weight-light">
                                                         <span class="font-weight-normal">${data.data.fileBName}</span> /
                                                        ${data.data.scanDateFileB}
                                                    </label>
                                                    <br>
                                                    <b>Total:</b>
                                                    <br>
                                                    <label class="btn btn-default text-center text-sm border border-dark">
                                                        <span class="text-sm">${data.data.difference.toFixed(2)} m³</span>
                                                    </label>
                                                </span>                                               
                                            </div>
                                        </div>`;
                            $("#wasteVolumeDiffAnalysisModalBody").append(html);
                            $("#wasteVolumeDiffAnalysisModal").modal('show');
                        }
                    }
                    else {
                        Swal.fire({
                            text: `@DbResHtml.T("No data received", "Resources")`,
                            confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                            allowOutsideClick: false,
                            icon: "error"
                        });
                    }
                   
                },
                error: function (xhr, status, error) {
                    Swal.fire({
                        icon: "error",
                        text: "@DbResHtml.T("Error occurred while processing your request", "Resources")"
                    });
                }
            });
        }
        function editFileConfirmed() {
            var id = $("#editPointCloudFilelIdInput").val();
            var filePath = $("#editPointCloudFilelPathInput").val();
            var name = $("#editPointCloudFilelNameInput").val();
            var scanDateTime = $("#editDateOfScan").data('date');
            var legalLandfillId = $("#editLegalLandfillSelect").val();
            var oldLegalLandfillId = $("#editOldLandfillId").val();

            var legalLandfillPointCloudFileViewModel = {
                Id: id,
                FileName: name,
                ScanDateTime: scanDateTime,
                LegalLandfillId: legalLandfillId,
                OldLegalLandfillid: oldLegalLandfillId,
                FilePath: filePath
            }

            $.ajax({
                type: "POST",
                url: "@Url.Action("EditLegalLandfillPointCloudFileConfirmed", "LegalLandfillsManagement", new { Area = "IntranetPortal" })",
                data: {
                    legalLandfillPointCloudFileViewModel: legalLandfillPointCloudFileViewModel
                },
                success: function (data) {
                    if (data != undefined || data != null) {
                        if (data.isSuccess == false && data.errMsg != null) {
                            Swal.fire({
                                text: `${data.errMsg}`,
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "error"
                            });
                        }
                        else {
                            Swal.fire({
                                text: '@DbResHtml.T("Successfully edited file", "Resources")',
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "success"
                            }).then((result) => {
                                if (result.isConfirmed) {
                                    location.reload();
                                }
                            });
                        }
                    }
                    else {
                        Swal.fire({
                            text: `@DbResHtml.T("No data received", "Resources")`,
                            confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                            allowOutsideClick: false,
                            icon: "error"
                        });
                    }
                },
                error: function (req, status, error) {
                    Swal.fire({
                        text: '@DbResHtml.T("Error occured", "Resources")',
                        icon: "error"
                    });
                }
            });
        }
        function editFileModalFunction(fileId) {
            $.ajax({
                type: "POST",
                url: "@Url.Action("GetLegalLandfillPointCloudFileById", "LegalLandfillsManagement", new { Area = "IntranetPortal" })",
                data: {
                    fileId: fileId
                },
                success: function (data) {
                    if (data != undefined || data != null) {
                        if (data.isSuccess == false && data.errMsg != null) {
                            Swal.fire({
                                text: `${data.errMsg}`,
                                confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                                allowOutsideClick: false,
                                icon: "error"
                            });
                        }
                        else {
                            $("#editLegalLandfillFileModalBody").html("");
                            var sdt = new Date(data.data.scanDateTime);
                            getAllLegalLandfills();

                            $("#editLegalLandfillFileModalBody").append(
                                '<div class="form-group">'
                                + '<input hidden class="form-control" id="editPointCloudFilelIdInput" />'
                                + '<input hidden class="form-control" id="editPointCloudFilelPathInput" />'
                                + '<input hidden class="form-control" id="editOldLandfillId" />'
                                + '<label class="form-control-label mt-3" for="editPointCloudFilelNameInput">@DbResHtml.T("Name:", "Resources")</label>'
                                + '<input type="text" name="EditFileName" class="form-control" id="editPointCloudFilelNameInput" required />'
                                + '</div>'
                                + '<div class="form-group">'
                                + '<label>@DbResHtml.T("Date of scan:", "Resources")</label>'
                                + '<div class="input-group date" id="editDateOfScan" data-target-input="nearest">'
                                + '<input type="text" class="form-control datetimepicker-input" data-target="#editDateOfScan" name="EditScanDateTime" />'
                                + '<div class="input-group-append" data-target="#editDateOfScan" data-toggle="datetimepicker">'
                                + '<div class="input-group-text"><i class="fa fa-calendar"></i></div>'
                                + '</div>'
                                + '</div>'
                                + '</div>'
                                + '<div class="form-group">'
                                + '<label>@DbResHtml.T("Select Legal Landfill:", "Resources")</label>'
                                + '<select id="editLegalLandfillSelect" class="form-control select2" style="width: 100%;" name="EditLegalLandfillId">'
                                + `<option selected value='${data.data.legalLandfill.id}'>${data.data.legalLandfill.name}</option>`
                                + '</select>'
                                + '</div>'
                            );

                            $("#editPointCloudFilelIdInput").val(data.data.id);
                            $("#editPointCloudFilelPathInput").val(data.data.filePath);
                            $("#editOldLandfillId").val(data.data.legalLandfillId);

                            $("#editPointCloudFilelNameInput").val(data.data.fileName);

                            $("#editLegalLandfillSelect").select2({
                                width: '100%',
                                theme: 'bootstrap4'
                            });

                            $('#editLegalLandfillFileModal').on('shown.bs.modal', function () {
                                $('#editDateOfScan').datetimepicker({
                                    format: 'L'
                                });
                                $('#editDateOfScan').datetimepicker('date', sdt);
                            });

                            $('#editLegalLandfillFileModal').modal('show');
                        }
                    }
                    else {
                        Swal.fire({
                            text: `@DbResHtml.T("No data received", "Resources")`,
                            confirmButtonText: '@DbResHtml.T("Ok", "Resources")',
                            allowOutsideClick: false,
                            icon: "error"
                        });
                    }
                },
                error: function (req, status, error) {
                    ajaxErrorHandlingAlert("error", req.status);
                }
            });
        }
        function deleteLandfillFileConfirmed() {
