@using MainApp.MVC.Helpers
@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration

<div class="w-100 mt-5">
    <div class="text-center mb-4">
        <div class="mb-3">
            <em class="fa fa-times-circle text-danger fa-5x"></em>
        </div>
        <div class="text-lg mb-3">404</div>
        <p class="lead m-0 text-danger">@DbResHtml.T("We couldn't find this map.", "Resources")</p>
        <p>@DbResHtml.T("MapViewComponent error! Cannot find requested map!", "Resources")</p>
    </div>
    <ul class="list-inline text-center text-sm mb-4">
        <li class="list-inline-item">
            <a class="btn bg-gradient-danger" href="~/@Configuration["DomainSettings:MainApp"]">@DbResHtml.T("Home", "Resources")</a>
        </li>
    </ul>
    <div class="p-3 text-center">
        <span class="mr-2">&copy;</span>
        <span>2023</span>
        <span class="mr-2">-</span>
        <span>UNICEF</span>
    </div>
</div>