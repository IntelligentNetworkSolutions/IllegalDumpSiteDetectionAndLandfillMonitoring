@using MainApp.Helpers;
@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration
@{
    Layout = "../Shared/_LayoutPage.cshtml";
    ViewData["Title"] = "Error";
}
<div class="abs-center wd-xl w-100">
    <!-- START card-->
    <div class="text-center mb-4">
        <div class="mb-3">
            <em class="fa fa-ban fa-5x text-info"></em>
        </div>        
        <div class="text-lg mb-3">403</div>
        <p class="lead m-0 text-info">@DbResHtml.T("Forbidden", "Resources") </p>
        <p>@DbResHtml.T("You are not authorized to view the web page which you are attempting to load.","Resources") </p>
    </div>
    <ul class="list-inline text-center text-sm mb-4">
        <li class="list-inline-item">
            <a class="btn bg-gradient-info" href="~/@Configuration["DomainSettings:MainApp"]">@DbResHtml.T("Home", "Resources")</a>
        </li>
    </ul>
    <div class="p-3 text-center">
        <span class="mr-2">&copy;</span>
        <span>2023</span>
        <span class="mr-2">-</span>
        <span>UNICEF</span>
    </div>
</div>