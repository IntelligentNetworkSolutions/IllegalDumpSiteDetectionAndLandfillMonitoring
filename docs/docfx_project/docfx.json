{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/images/**",
          "**/*.{svg,png,jpg,jpeg}"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}